{
  "name": "phonebook-backend",
  "version": "1.0.0",
  "description": "server for the phonebook app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
